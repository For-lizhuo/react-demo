{
  "name": "react-demo",
  "version": "0.0.1",
  "description": "A static site displaying React demos based on dumi.",
  "license": "MIT",
  "scripts": {
    "build": "dumi build",
    "deploy": "gh-pages -d docs-dist",
    "dev": "dumi dev",
    "prepare": "husky install && dumi setup",
    "start": "npm run dev"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "ahooks": "^3.7.8",
    "antd": "^5.12.1",
    "gh-pages": "^6.1.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "dumi": "^2.2.13",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "authors": [
    "lizhuo"
  ]
}
